{
    "manifest_version": 3,
    "name": "YouTube Search Date Filter",
    "version": "1.0.0",
    "description": "Filter search results by date",
    "permissions": ["storage"],
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*.google.com/search*",
                "*://*.youtube.com/*"
            ],
            "js": ["content.js"],
            "run_at": "document_end"
        }
    ]
}